{
  "name": "wink-ner",
  "version": "0.0.0",
  "description": "Named Entity Recognition",
  "keywords": [
    "NER",
    "Named Entity Recognition",
    "NLP",
    "wink"
  ],
  "main": "src/wink-ner.js",
  "scripts": {
    "pretest": "npm run lint && npm run hint && npm run docs",
    "test": "istanbul cover _mocha ./test/",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "sourcedocs": "docker -i src -o ./sourcedocs --sidebar no",
    "docs": "documentation build src/wink-ner.js -f html -o docs -c docs-toc.yml",
    "hint": "jshint ./src/*.js ./test/*.js ./runkit/*.js",
    "lint": "eslint ./src/*.js ./test/*.js ./runkit/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winkjs/wink-ner.git"
  },
  "author": "Sanjaya Kumar Saxena",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/winkjs/wink-ner/issues"
  },
  "homepage": "http://winkjs.org/",
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "docker": "^1.0.0",
    "documentation": "^5.3.5",
    "eslint": "^4.16.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.5",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "runkitExampleFilename": "./runkit/example.js"
}
